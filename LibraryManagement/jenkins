pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
               bat "rmdir  /s /q LibraryManagement"
                bat "git clone https://github.com/soniya1997/java/tree/main/LibraryManagement"
                bat "mvn clean -f LibraryManagement"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f LibraryManagement"
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f LibraryManagement"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f LibraryManagement"
            }
        }
    }
}